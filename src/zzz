t_channel			disruption_2(t_vec p,  t_vec center)
{
	int			u;
	int			v;
	t_channel	color;

	p -= center;
	p = normalize(p);
	u = 0;
	v = 0;
	u = ((sin(p.x * 2 * M_PI * 5) + 1) * 3.5) * 4;
	v = ((cos(p.y * 2 * M_PI * 5) + 1) * 3.5)*4;
	u *= v;
	color = (t_channel){(u >> 16) & 0xFF, (u >> 8) & 0xFF, (u & 0xFF)};
	if (color.r <= 25 && color.b <= 25 && color.g <= 25)
		color = (t_channel){0, 0, 0};
	else
		color = (t_channel){255, 255, 255};
	return (color);
}